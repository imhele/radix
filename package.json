{
  "name": "@imhele/radix",
  "version": "1.1.2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:es && npm run build:lib",
    "build:lib": "tsc --build tsconfig.json",
    "build:es": "tsc --build tsconfig.es.json",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "tslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "precommit": "lint-staged",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "keywords": [
    "radix"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imhele/radix"
  },
  "homepage": "https://github.com/imhele/radix",
  "authors": [
    "Hele <work@imhele.com> (https://github.com/imhele)"
  ],
  "bugs": {
    "url": "https://github.com/imhele/radix/issues"
  },
  "files": [
    "lib",
    "es"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "coveralls": "^3.0.2",
    "lint-staged": "^8.1.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "umi-test": "^1.3.0"
  }
}
